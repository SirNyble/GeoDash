        <item name="android:colorBackground">@color/background_light</item>
        <item name="android:colorPrimary">@color/bar</item>
        <item name="colorPrimaryDark">@color/background_light</item>
        <item name="android:statusBarColor">@color/holo_blue_dark</item>
        <item name="android:navigationBarColor">@color/holo_blue_dark</item>
        <item name="android:textColorPrimaryInverse">@android:color/primary_text_dark</item>
